{
  "name": "express-jwt-refreshtoken-template",
  "version": "0.0.1",
  "description": "User authentication template via JWT with Refresh Token for Express.js",
  "main": "lib/index.js",
  "scripts": {
    "init-admin": "babel-node src/scripts/install.js",
    "del-sessions": "babel-node src/scripts/autodelete_sessions.js",
    "start": "node .",
    "build": "babel src/ -d lib",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node src/index.js",
    "prebuild": "rimraf lib/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DarkMaper/express-jwt-refreshtoken-template.git"
  },
