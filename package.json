  "bugs": {
    "url": "https://github.com/DarkMaper/express-jwt-refreshtoken-template/issues"
  },
  "homepage": "https://github.com/DarkMaper/express-jwt-refreshtoken-template#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.20.0",
    "cors": "^2.8.5",
    "date-fns": "^2.27.0",
    "express": "^4.17.2",
    "express-useragent": "^1.0.15",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.2",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
